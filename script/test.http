#   "username": "test@qq.com",
#   "password": "123456",
#   "type": "teacher"
# }

{
  "username": "test1",
  "password": "123456",
  "type": "student"
}

### 老师注册
POST {{baseUrl}}/register
Accept: application/json
content-type: application/json

{
  "name": "test3",
  "email": "test3@qq.com",
  "password": "123456"
}

### 新增学生
POST {{baseUrl}}/students
Accept: application/json
content-type: application/json

{
  "name": "test1",
  "password": "123456",
  "school_id": 1
}

### test-token
GET {{baseUrl}}/teacher/info
Accept: application/json
content-type: application/json
Authorization: Bearer {{token}}

{
  "id": 1
}

