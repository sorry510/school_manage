{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "darkaonline/l5-swagger": "v5.5.*",
        "doctrine/dbal": "2.*",
        "encore/laravel-admin": "1.*",
        "fideloper/proxy": "~3.3",
        "fruitcake/laravel-cors": "v1.*",
        "laravel/framework": "5.5.*",
        "laravel/passport": "v4.0.3",
        "laravel/socialite": "v3.*",
        "laravel/tinker": "~1.0",
        "lcobucci/jwt": "3.3.3",
        "linecorp/line-bot-sdk": "^7.3",
        "socialiteproviders/line": "v2.*",
        "sorry510/annotation": "^0.0.1",
        "sorry510/constant": "^0.0.1",
        "workerman/phpsocket.io": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "v2.4.2",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "heroku/heroku-buildpack-php": "^212.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0",
        "zircote/swagger-php": "3.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd":[
            "php artisan cache:clear",
            "chmod -R 777 storage",
            "php artisan passport:keys",
            "chmod -R 777 bootstrap/cache",
            "php artisan config:cache",
            "php artisan route:cache"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "symfony/thanks": true
        }
    }
}
